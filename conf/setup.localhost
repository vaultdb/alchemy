#configure locations
codegen-target=bin
data-providers=conf/connections/localhosts

#log information
log-level=info

#query planner information
sliced-execution=true
slice-parallelism=2
semijoin-execution=true

#privacy consumed by each operator
epsilon=0.1
delta=0.0

#postgres credentials and info
[psql]
host=localhost
port=5432
db=smcql_testDB
user=smcql
password=<insert psql password>